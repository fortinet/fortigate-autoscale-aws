{
    "name": "fortigate-autoscale-aws",
    "version": "3.1.0-alpha.0",
    "description": "Fortinet FortiGate Autoscale for AWS.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "pre-commit": "npm-run-all lint-check audit-production test build",
        "audit-production": "npm audit --production",
        "compile": "tsc -d",
        "clear-dist": "rm -rf dist/{*,**/*}.d.ts",
        "build": "npm-run-all clear-dist compile",
        "make-dist": "node dist/scripts/make-dist.js",
        "build-artifacts": "npm-run-all build make-dist",
        "test": "mocha --timeout 3600000 -r ts-node/register test/*.spec.ts",
        "lint-check": "node node_modules/ftnt-devops-ci/bin/ftnt-devops-ci.js c -fl  -F .eslintignore -L .eslintignore \"{*,**/*}{.js,.json,.ts}\"",
        "lint-fix": "node node_modules/ftnt-devops-ci/bin/ftnt-devops-ci.js f -fl  -F .eslintignore -L .eslintignore \"**/*{.js,.json,.ts}\""
    },
    "keywords": [],
    "author": "Fortinet <github@fortinet.com>",
    "license": "MIT",
    "dependencies": {
        "@types/aws-lambda": "^8.10.46",
        "@types/node": "^13.9.2",
        "autoscale-core": "git+ssh://gerrit.fortinet.com:29418/devops/multi-cloud/autoscale/core#dev_testing_dependency_rev_2",
        "aws-sdk": "^2.657.0",
        "http-status-codes": "^1.4.0"
    },
    "devDependencies": {
        "@types/comment-json": "^1.1.1",
        "@types/mocha": "^7.0.2",
        "@types/shelljs": "^0.8.7",
        "@types/sinon": "^7.5.2",
        "@typescript-eslint/eslint-plugin": "^2.28.0",
        "@typescript-eslint/parser": "^2.28.0",
        "archiver": "^4.0.1",
        "chalk": "^3.0.0",
        "comment-json": "^3.0.2",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-mocha": "^6.3.0",
        "eslint-plugin-prettier": "^3.1.3",
        "extract-zip": "^2.0.0",
        "ftnt-devops-ci": "github:fortinet/ftnt-devops-ci",
        "mocha": "^7.1.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "shelljs": "^0.8.3",
        "sinon": "^9.0.2",
        "ts-node": "^8.8.2",
        "typescript": "^3.8.2"
    },
    "peerDependencies": {},
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/fortinet/autoscale-aws.git"
    },
    "bugs": {
        "url": "https://github.com/fortinet/autoscale-aws/issues"
    },
    "homepage": "https://github.com/fortinet/autoscale-aws#readme"
}
